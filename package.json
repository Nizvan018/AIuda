{
	"name": "proyecto",
	"version": "1.0.0",
	"description": "",
	"main": "index.ts",
	"scripts": {
		"build": "tsc",
		"clean": "rimraf dist",
		"start": "npm run build && npm run copy-files && node dist/index.js",
		"dev": "nodemon src/index.ts",
		"init": "npm install -D -s",
		"copy-files": "npm run copy-views && npm run copy-public",
		"copy-views": "copyfiles -u 1 \"./src/views/**/*.ejs\" \"dist\"",
		"copy-public": "copyfiles -u 1 \"./src/public/**/*.*\" \"dist\""
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"copyfiles": "^2.4.1",
		"ejs": "^3.1.8",
		"express": "^4.18.2",
		"express-session": "^1.17.3"
	  },
	  "devDependencies": {
		"@types/ejs": "^3.1.1",
		"@types/express": "^4.17.15",
		"@types/express-session": "^1.17.5",
		"nodemon": "^2.0.20",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4"
	  }
}